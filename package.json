    "ngx-toastr": "^19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "winston": "^3.18.3",
    "ws": "^8.18.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.18",
    "@angular/cli": "^19.2.4",
    "@angular/compiler-cli": "^19.2.18",
    "@aws-sdk/client-cloudformation": "^3.624.0",
