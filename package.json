{
  "name": "reserve-rec-admin",
  "version": "0.1.0",
  "bin": {
    "reserve-rec-admin": "bin/reserve-rec-admin.js"
  },
  "scripts": {
    "cdk": "cdk",
    "build": "ng build",
    "start": "ng serve",
    "synth:local": "cdk synth -c @context=local --all",
    "synth:dev": "cdk synth -c @context=dev --all",
    "synth:test": "cdk synth -c @context=test --all",
    "deploy:dev": "cdk deploy -c @context=dev --all",
    "deploy:test": "cdk deploy -c @context=test --all",
    "lint": "ng lint",
    "test": "jest",
    "test-ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "sandbox:setup": "./scripts/sandbox-setup.sh",
    "sandbox:teardown": "./scripts/sandbox-teardown.sh",
    "sandbox:synth": "cdk synth -c @context=dev -c sandboxName=${SANDBOX_NAME} --all",
    "sandbox:deploy": "cdk deploy -c @context=dev -c sandboxName=${SANDBOX_NAME} --all --require-approval never",
    "sandbox:destroy": "cdk destroy -c @context=dev -c sandboxName=${SANDBOX_NAME} --all --force"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.18",
    "@angular/common": "^19.2.16",
    "@angular/compiler": "^19.2.17",
